#include <stdio.h>
#include<iostream>

using namespace std;
int main()
{
int a[]={1,8,10}, b[]={2,5,9};
    int ans[6];
    int i = 0, j = 0, k = 0;
    while (i < 3 && j < 3)
    {
        if (a[i] < b[j])
        {
            ans[k] = a[i];
            i++;
        }
        else
        {
            ans[k] = b[j];
            j++;
        }
        k++;
    }

    while (i < 3)
    {
        ans[k] = a[i];
        i++;
        k++;
    }

    while (j < 3)
    {
        ans[k] = b[j];
        j++;
        k++;
    }
for (i=0;i<6;i++){
    cout<< ans[i];}
}
